/* Clouds left and right slowly */
.clouds.slow {
  -webkit-animation: slide 200s ease-out infinite;
  -moz-animation: slide 200s ease-out infinite;
  animation: slide 200s ease-out infinite;
}


@-webkit-keyframes slide
{
    0%   {left:0;}
    50% {left: -400px;}
    100% {left: 0;}
